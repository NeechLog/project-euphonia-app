# ============================================
# OAuth2.0 / OpenID Connect (OIDC) Configuration
# ============================================
# Copy this file to .env and update the values accordingly
# Keep this file secure and never commit sensitive data to version control
# OAuth configuration should be placed in files inside AUTH_CONFIG_DIR
AUTH_CONFIG_DIR = //Users/gagan/projects/work/project-euphonia-app/api/conf.d/

# JWT Configuration
JWT_SECRET=your-secret-key
JWT_EXPIRE_HOURS=24

# Admin Configuration
ADMIN_EMAILS=gagan@nukreatech.com,gagan.bajpai@gmail.com,gagan@tarqwork.com,pkatoch@nukreatech.com

# Storage Configuration
EUPHONIA_DIA_GCS_BUCKET=euphonia-dia
LOCAL_STORAGE_DIR=/path/to/local/storage
DATA_DIR=/path/to/data/directory

# Google Cloud Platform
PROJECT_ID=your-gcp-project-id
REGION=us-central1
ENDPOINT_ID=your-vertex-ai-endpoint-id

# Application Configuration
ENVIRONMENT=development
DEBUG=true
PYTHON_LOG_LEVEL=DEBUG
COOKIE_DOMAIN=localhost

# Testing
TESTING=true
DOTENV_PATH=/dev/null

# Model Configuration
TRANSCRIBE_MODEL=local
STORAGE=local
CLOUD=local

# Whisper Configuration
BASE_WHISPER_MODEL_TYPE=openai/whisper-small
LANGUAGE=en
MAX_NEW_TOKENS=256
CUSTOM_WHISPER_MODEL_PATH=custom_tiny_whisper_model

# Faster Whisper Configuration
faster_whisper_model_name_or_path=small
BEAM_SIZE=5

# DIA Model Configuration
GUIDANCE_SCALE_PARAM=4.0
TEMPERATURE_PARAM=1.8
TOP_P_PARAM=0.95
DEFAULT_SAMPLE_RATE=44100

# Vertex AI Model Configuration
config_SCALE_PARAM=0.3
DIADeployed=false
